/***************** Loop Controls *****************/
WBeatSpinBox,
#spinBoxTransition,
#LibraryBPMSpinBox  {
  selection-color: #000;
  selection-background-color: #d2d2d2;
  }
  WBeatSpinBox,
  #spinBoxTransition {
    border-width: 3px 19px 2px 3px;
    border-image: url(skin:/palemoon/buttons/spinbox_embedded_border.svg) 3 19 2 3;
    margin: 1px 0px 1px 1px;
    padding: 0px -17px 2px 1px;
    background-color: #0a0a0a;
    }
    WBeatSpinBox:focus,
    #spinBoxTransition:focus {
      border-image: url(skin:/palemoon/buttons/spinbox_embedded_border_focus_blue.svg) 3 19 2 3;
      background-color: #000;
    }
  WBeatSpinBox {
    margin: 1px 0px 1px 1px;
    padding: 0px -17px 2px 1px;
    background-color: #0a0a0a;
  }
  #spinBoxTransition {
    width: 24px;
    height: 19px;
    padding: 0px -15px 0px 0px;
    margin: 0px 2px 3px 5px;
  } /*
  WBeatSpinBox:hover,
  #spinBoxTransition:hover {
    background-color: #0a0a0a;
  } */
  WBeatSpinBox:focus,
  #spinBoxTransition:focus {
    background-color: #000;
  }

  WBeatSpinBox::up-button,
  WBeatSpinBox::down-button,
