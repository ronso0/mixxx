/* LateNight 2.3 // PaleMoon theme */


/******************************************************************************
************** Scheme-specific layout adjustments & colors ********************/

#Deck1, #Deck2, #Deck3,  #Deck4,
  #DeckCompact1, #DeckCompact2, #DeckCompact3, #DeckCompact4,
  #DeckMini1, #DeckMini2, #DeckMini3, #DeckMini4,
  #FxUnitControlsExpanded, #FxUnitControlsCollapsed,
  #FxSlotsExpanded, #FxSlotsCollapsed, #FxSlotContainer,
#MixerDecks,
#MicDuckingContainer, #MicAuxUnit, #MicAuxAddFrame,
#Sampler, #SamplerMini,
WEffectSelector::checked, #fadeModeCombobox::checked,
#ToolBar, #WaveformsContainer,
WLibrary,
#SidebarBox,
#PreviewDeck,
#SearchLineBox,
QAbstractScrollArea::corner { /*
  background-color: #1e1e1e;  */
  background-color: #1e1e20;
}

/* borders of elevated containers */
#Deck1, #Deck2, #Deck3, #Deck4,
  #DeckCompact1, #DeckCompact2, #DeckCompact3, #DeckCompact4,
  #DeckMini1, #DeckMini2, #DeckMini3, #DeckMini4,
#FxSlotsCollapsed, #FxSlotsExpanded,
#FxUnitControlsCollapsed, #FxUnitControlsExpanded,
#MixerDecks,
#MicDuckingContainer, #MicAuxUnit, #MicAuxAddFrame,
#Sampler, #SamplerMini,
WLibrary, #SidebarBox, #SearchLineBox, #PreviewDeck {
  border-top: 1px solid #333;
  border-left: 1px solid #282828;
  border-bottom: 1px solid #0c0c0c;
  border-right: 1px solid #181818;
  border-radius: 1px;
  border-bottom-left-radius: 2px;
}

#MixerDecks[highlight="1"] {
  border-top-right-radius: 0px;
  border-bottom-right-radius: 1px;
  border-right: 1px solid #0c0c0c;
}

#MixerMasterHeadphone {
  margin: 0px;
  }
  #MasterControls,
  #HeadphoneControls {  /*
    background-color: #151515;  */
    background-color: #151517;
    padding: 4px 4px 4px 4px;
    border-radius: 1px;
  }
  #MasterControls {
    border-top: 1px solid #333;
    border-left: 1px solid #282828;
    border-bottom: 1px solid #000;
    border-right: 1px solid #181818;
    border-top-right-radius: 1px;
  }
  #HeadphoneControls {
    border-top: 1px solid #282828;
    border-left: 1px solid #282828;
    border-bottom: 1px solid #000;
    border-right: 1px solid #181818;
    border-bottom-right-radius: 1px;
    border-bottom-left-radius: 0px;
  }

#FxParametersFocusBg {
  border: 1px solid #1c1c1e;
  background-color: rgba(0,0,1,50);
}

/* borders of embedded regions */
#OverviewBox, #OverviewBoxMini,
WSearchLineEdit {
  border-top: 1px solid #0d0d0d;
  border-left: 1px solid #121212;
  border-bottom: 1px solid #2a2a2a;
  border-right: 1px solid #252525;
  border-radius: 1px;
  background-color: #19191a;
}
#KeyText {
  border-top: 1px solid #0d0d0d;
  border-bottom: 1px solid #2a2a2a;
  background-color: #19191a;
}

#OverviewBox[highlight="1"],
#OverviewBoxMini[highlight="1"] {
  background-color: #151515;
}

#VuMeterContainerCompactDeck, #VuMeterContainerCompactMaster,
#FxUnitHeaderCollapsed,
#FxUnitHeaderExpanded,
#SamplerExpandBox {  /*  */
  border-top: 1px solid #212123;
  border-right: 1px solid #111;
  border-bottom: 1px solid #020202;
  border-left: 1px solid #191919;
  border-radius: 1px;
  background-color: #151517;
  }

#ToolBar[highlight="0"] {
  border-bottom: 1px solid #0c0c0c;
  }
  #ToolBar[highlight="1"] {
    border-bottom: 1px solid #1e1e20;
  }

/* background of fillers */
#FxRackFillerL,
#FxRackFillerR,
#MicAuxRackFiller {
  border-top: 1px solid #1c1c1c;
  border-left: 1px solid #191919;
  border-bottom: 1px solid #020202;
  border-right: 1px solid #111;
  border-radius: 1px;
  background-color: #151517;
  }
  #FxRackFillerL {
    margin: 0px 2px 3px 1px;
  }
  #FxRackFillerR {
    margin: 0px 1px 3px 2px;
  }

#VuMasterCover {
  background-color: rgba(21, 21, 21, 150);
}

WLibrary {
  border-top-left-radius: 0px;
}
#WaveformsBottomBorder, #WaveformsRightBorder {
  background-color: #333;
}
#WaveformsBottomBorder {
  padding-top: 1px;
}
#WaveformsContainer {
  padding: 0px 0px 1px 0px;
  border-bottom: 1px solid #0c0c0c;
  }
  #WaveformsContainer[highlight="0"] {
    padding: 0px 0px 1px 0px;
    border-width: 0px 0px 1px 0px;
    border-bottom-right-radius: 0px;
  }
  #WaveformsContainer[highlight="1"] {
    /* the right border is necessary only when the skin settings menu is visible */
    border-width: 0px 1px 1px 0px;
    border-bottom-right-radius: 1px;
  }

#SeparatorV,
#SeparatorFxV,
#SeparatorMixerV,
#SeparatorH,
#SeparatorFxH,
#SeparatorMixerH,
#SeparatorToolbar {
  /* needs a background color to paint the borders */
  background-color: transparent;
  }
  #SeparatorV,
  #SeparatorFxV /*,
  #SeparatorToolbar*/ {
    border-left: 1px solid #0c0c0c;
    border-right: 1px solid #333;
  }
  #SeparatorMixerV {
    border-left: 1px solid #0c0c0c;
    border-right: 1px solid #222;
  }
  #SeparatorFxV {
    margin: 1px 0px 1px 2px;
  }

  #SeparatorH,
  #SeparatorFxH {
    border-top: 1px solid #0c0c0c;
    border-bottom: 1px solid #333;
  }
  #SeparatorMixerH {
    border-top: 1px solid #000;
    border-bottom: 1px solid #222;
    /* needs a background color to paint the borders */
    background-color: transparent;
  }
  #SeparatorFxH {
    margin: 0px 2px;
  }
  #SeparatorToolbar {
    margin: 0px 5px;
  }

WLibrary,
#SearchLineBox {
  border-top: 0px;
}
#SearchLineBox {
  border-bottom-right-radius: 0px;
  border-right: 0px;
  border-top-right-radius: 0px;
  border-top-left-radius: 0px;
}
#SidebarBox {
  border-right: 0px;
  border-top-right-radius: 0px;
}


/************ separator lines within decks, samplers etc. *********************/
#DeckRow_1_BottomBorder, #DeckSettingsStars, #DeckRateSeparator,
#TitleText, #TitleTextSmall,
#PlayPositionText, #PlayPositionTextSmall,
#BpmTextSmall, #KeyTextSmall, #ArtistTextSmall, #PlayPositionTextSmall,
#PreviewPlayBox,
#SamplerTitle, #SamplerPlay, #SamplerSettings,
#SamplerGain, #SamplerVU, #SamplerPfl {
  border-style: solid;
  border-color: #0c0c0c;
}

/* see
  separator_artist_title.xml and
  separator_spinny_cover
  for visibility conditions */
#SeparatorLine {
  background-color: #0c0c0c;
}
